# AetherLink Environment Configuration

# Network Monitoring
# Set to 'true' to use real network data from your local network
# Set to 'false' to use mock/demo data
USE_REAL_NETWORK_DATA=false

# Your network prefix (e.g., 192.168.1, 192.168.0, 10.0.0)
NETWORK_PREFIX=192.168.1

# Optional: Router API Configuration
# Uncomment and configure based on your router type

# For UniFi Controller
# UNIFI_CONTROLLER_URL=https://192.168.1.1:8443
# UNIFI_USERNAME=admin
# UNIFI_PASSWORD=your_password
# UNIFI_SITE=default

# For ASUS Router
# ASUS_ROUTER_IP=192.168.1.1
# ASUS_USERNAME=admin
# ASUS_PASSWORD=your_password

# For TP-Link Router
# TPLINK_ROUTER_IP=192.168.1.1
# TPLINK_USERNAME=admin
# TPLINK_PASSWORD=your_password
